<!-- To get access to the model passed from the controller-->
@model TechAcadStudentsMVC.Models.Instructor


@{
    ViewBag.Title = "Instructor";
}

<h2>@ViewBag.Title</h2>

<h5>
    Details for Instructor with ID: @ViewBag.Id
</h5>

<p>
    Further details about the Instructor:
</p>

<!--Referencing the model passed from the controller-->
<ul>
    <li>Another Id: @Model.Id</li>
    <li>FirstName: @Model.FirstName</li>
    <li>LastName: @Model.LastName</li>
</ul>